    "from itertools import permutations\n",
    "\n",
    "count =0 \n",
    "data = ['A','B','C','D','E']\n",
    "perm = permutations(data,2)\n",
    "\n",
    "for i in list(perm):\n",
    "    print(i)\n",
    "    count+=1\n",
    "    \n",
    "\n",
    "    \n",
    "print(\"Permutation count:\",count)\n"
   ]
